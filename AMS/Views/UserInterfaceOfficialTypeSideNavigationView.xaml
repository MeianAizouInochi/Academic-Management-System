                        Width="auto"
                        Height="auto">

                        <Canvas
                            Name="dashboardIcon"
                            Width="24"
                            Height="24">

                            <Canvas.RenderTransform>

                                <TranslateTransform X="0" Y="0" />

                            </Canvas.RenderTransform>

                            <Canvas.Resources />

                            <!--  Unknown tag: sodipodi:namedview  -->

                            <Canvas Name="SVGRepo_bgCarrier" />

                            <Canvas Name="SVGRepo_tracerCarrier" />

                            <Canvas Name="SVGRepo_iconCarrier">

                                <Path
                                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                    Name="secondary"
